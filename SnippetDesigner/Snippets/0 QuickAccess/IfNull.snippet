<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>If Null</Title>
			<Author>HyperWave</Author>
			<Description>Inserts a null-check</Description>
			<Shortcut>ifn</Shortcut>
		</Header>
		<Snippet>
			<Code Language="CSharp"
				  Kind="method body">
				<![CDATA[if($selected$ == null)
				{
					$end$
				}
				]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>